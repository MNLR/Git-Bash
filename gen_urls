# Genera el listado de urls para descargar los ficheros automaticamente mediante wget

for num in `seq 2006 2014`
do
        echo http://www.minetad.gob.es/energia/balances/Publicaciones/ElectricasMensuales/${num}/{Enero,Febrero,Marzo,Abril,Mayo,Junio,Julio,Agosto,Septiempre,Octubre,Noviembre,Diciembre}/${num} | tr ' ' '\n'
done

echo http://www.minetad.gob.es/energia/balances/Publicaciones/ElectricasMensuales/2015/{Enero,Febrero,Marzo,Abril}

